apiVersion: v1
kind: Pod
metadata:
  name: rabbitmq-two
  labels:
    app: rabbitmq-two
spec:
  containers:
  - name: rabbitmq-two
    image: {{ docker_rabbitmq }}
    env:
      - name: HOSTNAME
        value: rabbitmq-two-svc
    ports:
      - containerPort: 4369
      - containerPort: 5671
      - containerPort: 5672
      - containerPort: 15672
      - containerPort: 25672
